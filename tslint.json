{
  "extends": "tslint:recommended",
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [true, "never"],
		"triple-equals": true,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "member-access": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "member-ordering": [
      false
    ]
	}
}